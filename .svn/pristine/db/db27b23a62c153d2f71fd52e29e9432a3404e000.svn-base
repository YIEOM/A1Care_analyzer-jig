<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background" >
    
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:src="@drawable/title" />
    
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="128px"
        android:layout_marginTop="20px"
        android:src="@drawable/time_title" />
    
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="531px"
        android:layout_marginTop="18px"
        android:src="@drawable/main_usb" />
    
   	<TextView
        android:id="@+id/timeText" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:typeface="sans"
        android:textSize="24px"
        android:textColor="#444444"
        android:shadowColor="#7F000000"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="1.2"
        android:layout_marginLeft="578px"
        android:layout_marginTop="15px" />
   	
   	<Button
        android:id="@+id/escicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="728px"
        android:background="@drawable/esc_selector" />
   	        
   	<ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="68px"
        android:layout_marginTop="240px"
        android:src="@drawable/time" />
   	
   	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
        android:layout_marginLeft="173px"
        android:layout_marginTop="81px" >
   	    
   	    <Button
	        android:id="@+id/ampmupbtn"
	        android:layout_width="129px"
	        android:layout_height="114px"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerInParent="true"
	        android:src="@drawable/date_up_icon" />
	        
   	</RelativeLayout>
   	
   	<TextView
   	    android:id="@+id/ampmtext"
   	    android:layout_width="129px"
        android:layout_height="wrap_content"
        android:layout_marginLeft="173px"
        android:layout_marginTop="216px"
        android:gravity="center"
        android:typeface="sans"
        android:textSize="60px"
        android:textColor="#565656" />
   		
   	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
      	android:layout_marginLeft="173px"
        android:layout_marginTop="321px" >
   	    
   	    <Button
	        android:id="@+id/ampmdownbtn"
	        android:layout_width="129px"
	        android:layout_height="114px"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerInParent="true"
	        android:src="@drawable/date_down_icon" />
	        
   	</RelativeLayout>
   	
   	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
        android:layout_marginLeft="361px"
        android:layout_marginTop="81px" >
   	    
   	    <Button
	        android:id="@+id/hplusbtn"
	        android:layout_width="129px"
	        android:layout_height="114px"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerInParent="true"
	        android:src="@drawable/date_up_icon" />
	        
   	</RelativeLayout>
	   	
   	<TextView
   	    android:id="@+id/hourtext"
   	    android:layout_width="129px"
        android:layout_height="wrap_content"
        android:layout_marginLeft="361px"
        android:layout_marginTop="216px"
        android:gravity="center"
        android:typeface="sans"
        android:textSize="60px"
        android:textColor="#565656" />
   		
   	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
        android:layout_marginLeft="361px"
        android:layout_marginTop="321px" >
   	    
   	    <Button
	        android:id="@+id/hminusbtn"
	        android:layout_width="129px"
	        android:layout_height="114px"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerInParent="true"
	        android:src="@drawable/date_down_icon" />
	        
   	</RelativeLayout>
	   	
   	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
        android:layout_marginLeft="553px"
        android:layout_marginTop="81px" >
   	    
   	    <Button
	        android:id="@+id/mplusbtn"
	        android:layout_width="129px"
	        android:layout_height="114px"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerInParent="true"
	        android:src="@drawable/date_up_icon" />
	        
   	</RelativeLayout>
   	
   	<TextView
   	    android:id="@+id/mintext"
   	    android:layout_width="129px"
        android:layout_height="wrap_content"
        android:layout_marginLeft="553px"
        android:layout_marginTop="216px"
        android:gravity="center"
        android:typeface="sans"
        android:textSize="60px"
        android:textColor="#565656" />
   	
   	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
        android:layout_marginLeft="553px"
        android:layout_marginTop="321px" >
   	    
   	    <Button
	        android:id="@+id/mminusbtn"
	        android:layout_width="129px"
	        android:layout_height="114px"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<ImageView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerInParent="true"
	        android:src="@drawable/date_down_icon" />
	        
   	</RelativeLayout>

</RelativeLayout>