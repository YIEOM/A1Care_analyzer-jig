<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/login_bg" >
    
    <EditText
        android:id="@+id/loginoid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="186px"
        android:layout_marginTop="200px"
        android:paddingLeft="20px"
        android:textSize="18px"
        android:textColor="#565656"
        android:hint="Operator"
        android:textColorHint="#E8E8E8"
        android:maxLength="10"
        android:singleLine="true" 
        android:imeOptions="actionDone"
        android:longClickable="false"
        android:background="@drawable/login_pass" />
    
    <EditText
        android:id="@+id/loginpass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="186px"
        android:layout_marginTop="253px"
        android:paddingLeft="20px"
        android:textSize="18px"
        android:textColor="#565656"
        android:hint="Password"
        android:textColorHint="#E8E8E8"
        android:maxLength="4"
        android:singleLine="true"
        android:inputType="numberPassword"
        android:imeOptions="actionDone"
        android:longClickable="false"
        android:background="@drawable/login_pass" />
    
   	<Button 
        android:id="@+id/logindbbtn"
        android:layout_width="48px"
        android:layout_height="42px"
        android:layout_marginLeft="472px"
        android:layout_marginTop="206px"
        android:background="@drawable/login_db" />
    
    <Button 
        android:id="@+id/loginbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="529px"
        android:layout_marginTop="201px"
        android:background="@drawable/login_selector" />
    
    <Button 
        android:id="@+id/checkbtn"
        android:layout_width="28px"
        android:layout_height="23px"
        android:layout_marginLeft="190px"
        android:layout_marginTop="306px"
        android:background="@drawable/login_checkbox" />
    	    
</RelativeLayout>