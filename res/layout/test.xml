<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/systemsettinglinear"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background" >
    
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:src="@drawable/title"
        android:background="#00000000" />
    
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="531px"
        android:layout_marginTop="18px"
        android:src="@drawable/main_usb"
        android:background="#00000000" />
    
   	<TextView
        android:id="@+id/timeText" 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:typeface="sans"
        android:textSize="24px"
        android:textColor="#444444"
        android:shadowColor="#7F000000"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="1.2"
        android:layout_marginLeft="578px"
        android:layout_marginTop="15px" />
   		
   	<Button
        android:id="@+id/escicon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="728px"
        android:background="@drawable/esc_selector" />
   	   	
   	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginLeft="30px"
        android:layout_marginTop="134px" >
   	    
   	    <Button
	        android:id="@+id/labviewbtn"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
	        android:layout_marginTop="85px" 
	        android:layout_centerHorizontal="true"
	        android:gravity="center"
		    android:text="LAB VIEW"
	        android:typeface="sans"
	        android:textSize="18px"
	        android:textColor="#242424" />
		    
   	</RelativeLayout>

 	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginLeft="182px"
        android:layout_marginTop="134px" >
   	    
   	    <Button
	        android:id="@+id/stabilitybtn"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
	        android:layout_marginTop="85px" 
	        android:layout_centerHorizontal="true"
	        android:gravity="center"
		    android:text="STABILITY"
	        android:typeface="sans"
	        android:textSize="18px"
	        android:textColor="#242424" />
		    
   	</RelativeLayout>

 	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginLeft="335px"
        android:layout_marginTop="134px" >
   	    
   	    <Button
	        android:id="@+id/tempbtn"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
	        android:layout_marginTop="85px" 
	        android:layout_centerHorizontal="true"
	        android:gravity="center"
		    android:text="TEMPERATURE"
	        android:typeface="sans"
	        android:textSize="18px"
	        android:textColor="#242424" />
		    
   	</RelativeLayout>

 	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginLeft="486px"
        android:layout_marginTop="134px" >
   	    
   	    <Button
	        android:id="@+id/phototempbtn"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
	        android:layout_marginTop="75px" 
	        android:layout_centerHorizontal="true"
	        android:gravity="center"
	        android:text="PHOTO &amp;\nTEMPERATURE"
	        android:typeface="sans"
	        android:textSize="18px"
	        android:textColor="#242424" />
		    
   	</RelativeLayout>			
   	
 	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginLeft="639px"
        android:layout_marginTop="134px" >
   	    
   	    <Button
	        android:id="@+id/photobtn"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
	        android:layout_marginTop="85px" 
	        android:layout_centerHorizontal="true"
	        android:gravity="center"
	        android:text="PHOTO"
	        android:typeface="sans"
	        android:textSize="18px"
	        android:textColor="#242424" />
		    
   	</RelativeLayout>
 	
 	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginLeft="30px"
        android:layout_marginTop="269px" >
   	    
   	    <Button
	        android:id="@+id/photoabsbtn"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
	        android:layout_marginTop="70px" 
	        android:layout_centerHorizontal="true"
	        android:gravity="center"
	        android:text="PHOTO &amp;\nABSORBANCE"
	        android:typeface="sans"
	        android:textSize="18px"
	        android:textColor="#242424" />
		    
   	</RelativeLayout>
   	
 	<RelativeLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginLeft="182px"
        android:layout_marginTop="269px" >
   	    
   	    <Button
	        android:id="@+id/cbtn"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:background="@drawable/systemsetting_btn_selector" />
   	    
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
	        android:layout_marginTop="70px" 
	        android:layout_centerHorizontal="true"
	        android:gravity="center"
	        android:typeface="sans"
	        android:textSize="18px"
	        android:textColor="#242424" />
		    
   	</RelativeLayout>
 	
 	<Button
        android:id="@+id/nbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
  	    android:layout_marginLeft="335px"
        android:layout_marginTop="269px"
        android:gravity="center"
        android:typeface="sans"
        android:textSize="18px"
        android:textColor="#242424"
        android:background="@drawable/systemsetting_btn_selector" />
    
    <Button
        android:id="@+id/pbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
  	    android:layout_marginLeft="486px"
        android:layout_marginTop="269px"
        android:gravity="center"
        android:typeface="sans"
        android:textSize="18px"
        android:textColor="#242424"
        android:background="@drawable/systemsetting_btn_selector" />
    
   	<Button
        android:id="@+id/tbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
  	    android:layout_marginLeft="639px"
        android:layout_marginTop="269px"
        android:gravity="center"
        android:typeface="sans"
        android:textSize="18px"
        android:textColor="#242424"
        android:background="@drawable/systemsetting_btn_selector" />
    	        
</RelativeLayout>